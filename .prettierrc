{
  "overrides": [
    {
      "files": "*.{js,jsx,ts,tsx,jsx,css,json,html}",
      "options": {
        "bracketSpacing": true,
        "endOfLine": "crlf",
        "jsxBracketSameLine": false,
        "jsxSingleQuote": false,
        "printWidth": 120,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "all",
        "useTabs": true,
        "tabWidth": 4
      }
    }
  ]
}
