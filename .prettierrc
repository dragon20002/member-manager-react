{
  "overrides": [
    {
      "files": "*.{js,jsx,ts,tsx,jsx}",
      "options": {
        "bracketSpacing": true,
        "endOfLine": "crlf",
        "jsxBracketSameLine": false,
        "jsxSingleQuote": false,
        "printWidth": 100,
        "semi": true,
        "singleQuote": true,
        "tabWidth": 2,
        "trailingComma": "all",
        "useTabs": false
      }
    }
  ]
}
